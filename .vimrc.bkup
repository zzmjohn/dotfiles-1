execute pathogen#infect()
syntax on
filetype plugin indent on
let g:Powerline_symbols = 'fancy'

colorscheme delek

set laststatus=2
set encoding=utf-8
set fillchars+=stl:\ ,stlnc:\

set expandtab
set list
set listchars=eol:~
set ls=2
set modeline
set nocompatible
set number
set shiftwidth=2
set smartindent
set statusline+=%F
set tabstop=2
